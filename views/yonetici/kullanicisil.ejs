                        <li>
                          <hr class="dropdown-divider">
                        </li>
                      </ul>
                    </li>
                    <li><a class="nav-link" href="/cikis">Çıkış</a></li>
                </ul>
              </div>
            </div>
        </div>
    </nav>
    <nav class="main-menu">
        <ul>
            <li class="has-subnav">
                <a href="/admin/kullanicitablosu">
                    <i class="fa fa-2x bi bi-arrow-right"></i>
                    <span class="nav-text">
                        Kullanıcıları Görün
                    </span>
                </a>
                
            </li>
            <li class="has-subnav">
                <a href="/admin/kullaniciekle">
                    <i class="fa fa-2x bi bi-arrow-right"></i>
                    <span class="nav-text">
                        Yeni Kullanıcı Ekleyin
                    </span>
                </a>
                
            </li>
            <li class="has-subnav">
                <a href="/admin/kullaniciguncelle">
                    <i class="fa fa-2x bi bi-arrow-right"></i>
                    <span class="nav-text">
